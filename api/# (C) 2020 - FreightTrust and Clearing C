  /api/v1/partner/list:
    post:
      tags:
        - Partner
      summary: Gets a list of partner records.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  default: id
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                search_after:
                  type: array
                  description: 'Array of key/values to get the next set of result after. [optional]'
                  example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                access_permission:
                  type: string
                  description: 'Access permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: public
                manage_permission:
                  type: string
                  description: 'Manage permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: company
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of control numbers.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
                    properties:
                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      search_after_values:
                        type: array
                        description: 'Key/Value pairs for the last record, used to search for the next page of results.'
                        example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                  results:
                    type: array
                    description: List of control number records.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: ID of the record.
                          example: hdP13WwBKlTkYuo1LBqy
                        idpartner:
                          type: string
                          description: ID of the partner to reference.
                          example: TESTPARTNER1
                        desc:
                          type: string
                          description: Partner description.
                          example: Test Partner Company Information.
                        mail:
                          type: string
                          description: Email address.
                          example: name@partner.com
                        cc:
                          type: string
                          description: Additional email address.
                          example: name@partner.com
                        phone1:
                          type: string
                          description: Phone number.
                          example: 123-123-8888
                        phone2:
                          type: string
                          description: Second phone number.
                          example: 321-321-7777
                        startdate:
                          type: string
                          description: Second phone number.
                          example: '2019-06-01'
                        enddate:
                          type: string
                          description: Second phone number.
                          example: '2025-01-31'
                        name:
                          type: string
                          description: Name
                          example: Test Partner
                        name1:
                          type: string
                          description: Name 1
                          example: ''
                        name2:
                          type: string
                          description: Name 2
                          example: ''
                        name3:
                          type: string
                          description: Name 3
                          example: ''
                        address1:
                          type: string
                          description: Street Address 1
                          example: 789 Ave.
                        address2:
                          type: string
                          description: Street Address 2
                          example: Apt. 2
                        address3:
                          type: string
                          description: Street Address 3
                          example: 'Attn: John Smith'
                        city:
                          type: string
                          description: City Name
                          example: Miami
                        countrysubdivision:
                          type: string
                          description: 'State, Province, Country sub division'
                          example: FL
                        postalcode:
                          type: string
                          description: Zip or Postal Code
                          example: '33101'
                        countrycode:
                          type: string
                          description: Country Code
                          example: US
                        attr1:
                          type: string
                          description: Additional attribute 1
                          example: Vendor Number
                        attr2:
                          type: string
                          description: Additional attribute 2
                          example: VAT ID
                        attr3:
                          type: string
                          description: Additional attribute 3
                          example: 'NET: 30'
                        attr4:
                          type: string
                          description: Additional attribute 4
                          example: ''
                        attr5:
                          type: string
                          description: Additional attribute 5
                          example: ''
