openapi: 3.0.0
info:
  version: "1.0.0"
  title: Freight Trust and Clearing EDI.ZONE
  description: 'RESTful API Service for translating data from one format to another.'
  termsOfService: 'https://github.com/freight-trust/legal/terms-of-service.md'
security:
  - ApiKeyAuth: []
  - BasicAuth: []
tags:
  - name: Company
    description: The company of the authorized user.
  - name: Control Numbers
    description: 'Used as increment counters primarily in outbound EDIFACT, TRADACOMS, and X12 control numbers.'
  - name: Conversion Codes
    description: 'Used for converting qualifiers and codes.<br/>Example: EA Unit of measure code to ''Each'' text.'
  - name: Convert
    description: 'Uses translation maps to convert data or create acknowledgements.<br/>Standard data types include CSV, EDIFACT, EXCEL, FIXED, HTML, IDOCS, JSON, PDF, TRADACOMS, WORD, X12, XML.'
  - name: Grammar
    description: Descriptive file structure definition used in lexical analysis.
  - name: Log
    description: Process Log information for data conversion.
  - name: Partner
    description: Partner or company information including address.
  - name: Translation Map
    description: Program that converts data from one format to another.
  - name: User
    description: Authorized user of the api.
  - name: Utilities
    description: Helper functions for the api.<br/>Primarily for converting between text stings and base64.
paths:
  /api/v1/:
    get:
      tags:
        - Utilities
      summary: EDI.ZONE API Information
      security: {}
      responses:
        '200':
          description: '(OK) Successfully processed, return API information object.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  program:
                    type: string
                    description: The name of the API.
                    example: EDI.ZONE
                  version:
                    type: string
                    description: The version of the API.
                    example: 1.0.0
                  status:
                    type: string
                    description: The status of the API.
                    example: active
                  message:
                    type: string
                    description: The status message.
                    example: ok
  /api/v1/user:
    get:
      tags:
        - Utilities
      summary: Authorized User Information
      responses:
        '200':
          description: '(OK) Successfully processed, return Authorized User Object.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  user_id:
                    type: string
                    description: The ID of the authorized user.
                    example: jx8Yq2wOMt0esONO7uLx
                  user_name:
                    type: string
                    description: The name of the authorized user.
                    example: John Doe
                  company:
                    type: string
                    description: The company the authorized user belongs to.
                    example: FreightTrust & Clearing Corporation
                  company_id:
                    type: string
                    description: The Company ID of the authorized user.
                    # TODO : CHANGE TO SPEC (mneomic)
                    example: 
                  trading_channel_id:
                    type: string
                    description: The trading channel the authorized user belongs to. (0 = no network)
                    example: FreightTrust & Clearing Corporation
                  auth_level:
                    type: string
                    description: The authorization level the authorized user has.
                    example: MailboxAdmin
  /api/v1/file2base64:
    post:
      tags:
        - Utilities
      summary: Converts an uploaded file to a Base64 string.
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties:
                file:
                  type: string
                  format: binary
                  description: 'The file to convert, allowed file types: txt, x12, edifact, edi, csv, json, html'
      responses:
        '200':
          description: '(OK) Successfully processed, return Base64 string representation of the file.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  result:
                    type: string
                    format: base64
                    description: Base64 encoded character string.
                    example: IyBtYXBwaW5nLXNjcmlwdA0KaW1wb3J0IGJvd...
  /api/v1/string2base64:
    post:
      tags:
        - Utilities
      summary: Converts a text string to a base64 string.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                content:
                  type: string
                  description: The text string.
                  example: ISA*00* *00* *ZZ*9339687055968 *ZZ*9377778093925 *200511*0659*U*00200*000543539*0*P*>
      responses:
        '200':
          description: '(OK) Successfully processed, return the base64 encoded string.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  result:
                    type: string
                    description: The Base64 encoded string.
                    example: SVNBKjAwKiAqMDAqICpaWio5MzM5Njg3MDU1OTY4ICpaWio5Mzc3Nzc4MDkzOTI1ICoyMDA1MTEqMDY1OSpVKjAwMjAwKjAwMDU0MzUzOSowKlAqPg
  /api/v1/base642file:
    post:
      tags:
        - Utilities
      summary: Converts a Base64 string to a file.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                filename:
                  type: string
                  description: The name of the file to create.
                  example: https://github.com/freight-trust/libedi/blob/v1.3.0/lib/00603/X12_00603_100.ts
                content:
                  type: string
                  format: base64
                  description: The base64 encoded string representation of the file
                  example: InVzZSBzdHJpY3QiOwp2YXIgdmFsaWRhdGUgPSAoZnVuY3Rpb24gKCkgewogIHZhciByZW
      responses:
        '200':
          description: '(OK) Successfully processed, return the file.'
          content:
            application/octet-stream:
              schema:
                type: file
                format: binary
  /api/v1/base642string:
    post:
      tags:
        - Utilities
      summary: Converts a Base64 string to a text string.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                content:
                  type: string
                  format: base64
                  description: The base64 encoded string to convert.
                  example: IyBtYXBwaW5nLXNjcmlwdA0KaW1wb3J0IGJvd...
      responses:
        '200':
          description: '(OK) Successfully processed, return text string.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  result:
                    type: string
                    description: Text string.
                    example: ISA*00* *00* *ZZ*9339687055968 *ZZ*9377778093925 *200511*0659*U*00200*000543539*0*P*>
  /api/v1/codes/list:
    post:
      tags:
        - Conversion Codes
      summary: Gets a list of conversion code triggers or code list names.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  default: id
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                search_after:
                  type: array
                  description: 'Key/values pairs used to search for the next page of results. [optional]'
                  example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                access_permission:
                  type: string
                  description: 'Access permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: public
                manage_permission:
                  type: string
                  description: 'Manage permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: company
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of conversion code triggers or code list names.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
                    properties:
                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                      search_after_values:
                        type: array
                        description: 'Key/Value pairs for the last record, used to search for the next page of results.'
                        example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                  results:
                    type: array
                    description: List of conversion code triggers or code list names.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: ID of the conversion code record.
                          # TODO : CHANGE TO SPEC
                          example: 
                        ccode_id_trigger:
                          type: string
                          description: Name of the conversion code record.
                          example: ISO_UOM_TO_TEXT
                        ccodeid_trigger_description:
                          type: string
                          description: Description of the conversion code record.
                          example: ISO Unit of Measurement to Text
  /api/v1/codes:
    post:
      tags:
        - Conversion Codes
      summary: Add a conversion code trigger or code list.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                ccode_id_trigger:
                  type: string
                  description: Name of the code trigger record name.
                  example: ISO_PRICETYPE_TO_TEXT
                ccodeid_trigger_description:
                  type: string
                  description: 'Number of records to return. [optional]'
                  example: ISO Pricetype to Text
                ccodes:
                  type: array
                  description: List of code conversions.
                  items:
                    type: object
                    properties:
                      leftcode:
                        type: string
                        description: Left the conversion code or origin text.
                        example: CA
                      rightcode:
                        type: string
                        description: Right the conversion code or converted text.
                        example: Catalogue
                permissions:
                  type: object
                  description: Permissions to assign to the record.
                  properties:
                    access:
                      type: string
                      description: Access permission level for the record.
                      example: public
                      oneOf:
                        - public
                        - network
                        - company
                        - user
                    manage:
                      type: string
                      description: Manage permission level for the record.
                      example: user
                      oneOf:
                        - public
                        - network
                        - company
                        - user
      responses:
        '201':
          description: '(CREATED) Successfully created, returns conversion code trigger add result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    # TODO : CHANGE TO SPEC
                    example: 
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: created
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
  '/api/v1/codes/{id}':
    parameters:
      - name: id
        in: path
        description: ID of the conversion code that we want to match
        type: string
    get:
      tags:
        - Conversion Codes
      summary: Get record with given ID. Must have appropriate access permission.
      parameters:
        - in: path
          name: id
          description: Record  ID
          schema:
            $ref: '#/components/parameters/id'
      responses:
        '200':
          description: '(OK) Successfully created, returns add result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  ccode_id_trigger:
                    type: string
                    description: Name of the code trigger record name.
                    example: ISO_PRICETYPE_TO_TEXT
                  ccodeid_trigger_description:
                    type: string
                    description: Number of records to return
                    example: ISO Pricetype to Text
                  ccodes:
                    type: array
                    description: List of code conversions.
                    items:
                      type: object
                      properties:
                        leftcode:
                          type: string
                          description: Left the conversion code or origin text.
                          example: CA
                        rightcode:
                          type: string
                          description: Right the conversion code or converted text.
                          example: Catalogue
                  permissions:
                    type: object
                    description: Permissions to assign to the record.
                    properties:
                      access:
                        type: string
                        description: Access permission level for the record.
                        example: public
                        oneOf:
                          - public
                          - network
                          - company
                          - user
                      manage:
                        type: string
                        description: Manage permission level for the record.
                        example: user
                        oneOf:
                          - public
                          - network
                          - company
                          - user
    put:
      summary: Edit/Update a record by ID. Must have appropriate access permission.
      tags:
        - Conversion Codes
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      requestBody:
        description: Conversion Code Object
        content:
          application/json:
            schema:
              type: object
              properties:
                ccode_id_trigger:
                  type: string
                  description: 'Name of the code trigger record name. [optional]'
                  example: ISO_PRICETYPE_TO_TEXT
                ccodeid_trigger_description:
                  type: string
                  description: 'Number of records to return. [optional]'
                  example: ISO Pricetype to Text
                ccodes:
                  type: array
                  description: 'List of code conversions. [optional]'
                  items:
                    type: object
                    properties:
                      leftcode:
                        type: string
                        description: Left the conversion code or origin text.
                        example: CA
                      rightcode:
                        type: string
                        description: Right the conversion code or converted text.
                        example: Catalogue
                permissions:
                  type: object
                  description: 'Permissions to assign to the record. [optional]'
                  properties:
                    access:
                      type: string
                      description: 'Access permission level for the record. [optional]'
                      example: public
                      oneOf:
                        - public
                        - network
                        - company
                        - user
                    manage:
                      type: string
                      description: 'Manage permission level for the record. [optional]'
                      example: user
                      oneOf:
                        - public
                        - network
                        - company
                        - user
      responses:
        '202':
          description: '(ACCEPTED) Successfully updated, returns updated result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    # TODO : CHANGE TO SPEC
                    example: 
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: updated
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
    delete:
      summary: Delete record by ID. Must have appropriate access permission.
      tags:
        - Conversion Codes
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      responses:
        '200':
          description: '(OK) Successfully deleted, returns deleted result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    # TODO : CHANGE TO SPEC
                    example:
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: deleted
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
  /api/v1/company/me:
    get:
      tags:
        - Company
      summary: Gets your company information.
      responses:
        '200':
          description: '(OK) Successfully created, returns add result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the company.
                    # TODO : CHANGE TO SPEC
                    example: 
                  name:
                    type: string
                    description: The name of the company.
                    example: Company
                  industry:
                    type: string
                    description: The industry of the company.
                    example: Technology
                  contact:
                    type: string
                    description: The contact name for the company.
                    example: Manager
                  email:
                    type: string
                    description: The contact email for the company.
                    example: name1@company.com
                  phone:
                    type: string
                    description: The phone number of the authorized user.
                    example: 555-123-4567
                  address1:
                    type: string
                    description: Street Address 1
                    example: 789 Ave.
                  address2:
                    type: string
                    description: Street Address 2
                    example: Apt. 2
                  address3:
                    type: string
                    description: Street Address 3
                    example: 'Attn: John Smith'
                  city:
                    type: string
                    description: City Name
                    example: Miami
                  countrysubdivision:
                    type: string
                    description: 'State, Province, Country sub division'
                    example: FL
                  postalcode:
                    type: string
                    description: Zip or Postal Code
                    example: '33101'
                  countrycode:
                    type: string
                    description: Country Code
                    example: US
                  billing:
                    type: object
                    description: Billing information.
                    properties:
                      credits:
                        type: integer
                        description: Credits available for translation.
                        example: 10
                      total_credits_used:
                        type: string
                        description: Total credits used for translation.
                        example: 2445
                      trial_credits:
                        type: string
                        description: Trial credits available for translation.
                        example: 4
