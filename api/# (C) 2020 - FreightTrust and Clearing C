  /api/v1/grammar/htmltemplate:
    post:
      tags:
        - Grammar
      summary: Add a htmltemplate grammar from a base64 string
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                name:
                  type: string
                  description: 'Name of the grammar. [optional]'
                  example: Order HTML Template Envelope
                companyname:
                  type: string
                  description: 'Company for the grammar. [optional]'
                  example: ''
                description:
                  type: string
                  description: 'Description of the grammar this record belongs to. [optional]'
                  example: Order Genshi HTML Envelope Template Grammar
                documenttype:
                  type: string
                  description: 'Type of document for this grammar. [optional]'
                  example: html
                version:
                  type: string
                  description: 'Version for the grammar. [optional]'
                  example: '1'
                formattype:
                  type: string
                  description: Document format of the grammar.
                  example: templatehtml
                messagetype:
                  type: string
                  description: Name of the grammar message type.
                  example: orders2print
                content:
                  type: string
                  format: base64
                  description: Base64 encode string of the grammar.
                  example: 
                permissions:
                  type: object
                  description: Permissions to assign to the record.
                  properties:
                    access:
                      type: string
                      description: Access permission level for the record.
                      example: public
                      oneOf:
                        - public
                        - network
                        - company
                        - user
                    manage:
                      type: string
                      description: Manage permission level for the record.
                      example: user
                      oneOf:
                        - public
                        - network
                        - company
                        - user
      responses:
        '201':
          description: '(CREATED) Successfully created, returns operation result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: 
                  result:
                    type: string
