  /api/v1/convert/xml2grammar:
    post:
      tags:
        - Convert
      summary: Converts/Translates a xml string into a grammar string. (For use as a starting template only.)
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                all_records:
                  type: integer
                  format: int32
                  description: 'If there are multiple records, Flag to create a grammar for each. [optional]'
                  example: 0
                  default: 0
                base64:
                  type: integer
                  format: int32
                  description: 'Flag to mark string as base64 or not. [optional]'
                  example: '0'
                  default: '0'
                in_data:
                  type: string
                  description: Inbound data to create a grammar for.
                  example: <?xmlversion="1.0" encoding="utf-8"?><message><sender>PARTNERB</sender>...
      responses:
        '200':
          description: '(OK) Successfully processed, returns data conversion results.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  count:
                    type: integer
                    format: int32
                    description: Count of converted objects.
                    example: 1
                  data:
                    type: array
                    description: List of grammars created.
                    items:
                      type: object
                      description: Created grammar.
                      properties:
                        content:
                          type: string
                          format: base64
                          description: Base64 encoded string of the grammar.
                          # TODO : GRAMMAR SPEC, ABNF IN GITHUB.COM/FREIGHT-TRUST/PROTOCOL/abnf
                          example: 
                        text_content:
                          type: string
                          description: Text string of the grammar.
                          example: Provided By <RESOURCE>
                        success:
                          type: boolean
                          description: If the operation was successful.
                          example: true
                  log_id:
                    type: string
                    description: Log ID record of the translation process.
                    # TODO : NETWORK UUID RECEIPT 
                    example: '0xb33e32b6f67c8ab12e3e6df8773b686def3499ab474358856616d8f6d3755ca'
