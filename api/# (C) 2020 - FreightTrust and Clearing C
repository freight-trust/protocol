  /api/v1/control-number:
    get:
      tags:
        - Control Numbers
      summary: Get a record for your company. Must have appropriate permission.
      responses:
        '200':
          description: '(OK) Success, returns the result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  company_id:
                    type: string
                    description: ID of the company this control number record belongs to.
                    example: tfryju45BKlTkYuo1LBqy
                  company_name:
                    type: string
                    description: Name of the company this control number record belongs to.
                    example: FreightTrust & Clearing Corporation
                  csv_control_number:
                    type: object
                    description: Values of control numbers for csv records.
                    properties:
                      general:
                        type: integer
                        format: int32
                        description: Current/default control number.
                        example: 1
                      parter_id:
                        type: integer
                        format: int32
                        description: Current control number for a specific partner.
                        example: 0
                  edifact_control_number:
                    type: object
                    description: Values of control numbers for edifact records.
                    properties:
                      general:
                        type: integer
                        format: int32
                        description: Current/default control number.
                        example: 1
                      parter_id:
                        type: integer
                        format: int32
                        description: Current control number for a specific partner.
                        example: 0
                  fixed_control_number:
                    type: object
                    description: Values of control numbers for fixed position records.
                    properties:
                      general:
                        type: integer
                        format: int32
                        description: Current/default control number.
                        example: 1
                      parter_id:
                        type: integer
                        format: int32
                        description: Current control number for a specific partner.
                        example: 0
                  idoc_control_number:
                    type: object
                    description: Values of control numbers for idoc position records.
