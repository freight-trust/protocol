  /api/v1/translation-map/list:
    post:
      tags:
        - Translation Map
      summary: Gets a list of translation maps.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  default: id
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                search_after:
                  type: array
                  description: 'Array of key/values to get the next set of result after. [optional]'
                  example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                access_permission:
                  type: string
                  description: 'Access permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: public
                manage_permission:
                  type: string
                  description: 'Manage permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: company
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of translation maps.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
                    properties:
                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      search_after_values:
                        type: array
                        description: 'Key/Value pairs for the last record, used to search for the next page of results.'
                        example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                  results:
                    type: array
                    description: List of grammar records.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: ID of the record.
                          example: hdP13WwBKlTkYuo1LBqy
                        companyname:
                          type: string
                          description: Company for the translation map.
                          example: ABC Corp.
                        partnername:
                          type: string
                          description: Partner for the translation map.
                          example: Partner 2
                        description:
                          type: string
                          description: Description of the translation map this record belongs to.
                          example: Converts ERP Order CSV File to X12 850 004010
                        documenttype:
                          type: string
                          description: Type of document for this translation map.
                          example: Translation map
                        version:
                          type: string
                          description: Version for the translation map.
                          example: '1.0'
                        from_formattype:
                          type: string
                          description: Document format of the grammar to translate from.
                          example: csv
                        from_messagetype:
                          type: string
                          description: Name of the grammar message type to translate from.
                          example: order_csv
                        frompartner_id:
                          type: string
                          description: Name of the partner to translate from.
                          example: ''
                        to_formattype:
                          type: string
                          description: Document format of the grammar to translate to.
                          example: csv
                        to_messagetype:
                          type: string
                          description: Name of the grammar message type to translate to.
                          example: order_csv
                        topartner_id:
                          type: string
                          description: Name of the partner to translate to.
                          example: ''
                        tscript:
                          type: string
                          description: Name of the translation map.
                          example: ordercsv_2_x12850
  /api/v1/translation-map/search:
    post:
      tags:
        - Translation Map
      summary: Search translation maps and return a list.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  default: id
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                tscript:
                  type: string
                  description: 'Name of the translation map. [optional] - wildcard character ''*'''
                  example: ordercsv_2_x12*
                from_formattype:
                  type: string
                  description: 'From format type of grammar for translation. [optional] - wildcard character ''*'''
                  example: csv
                from_messagetype:
                  type: string
                  description: 'From message type of grammar for translation. [optional] - wildcard character ''*'''
                  example: order*
                to_formattype:
                  type: string
                  description: 'To format type of grammar for translation. [optional] - wildcard character ''*'''
                  example: x12
                to_messagetype:
                  type: string
                  description: 'To message type of grammar for translation. [optional] - wildcard character ''*'''
                  example: 850*
                documenttype:
                  type: string
                  description: 'Document type for translation. [optional] - wildcard character ''*'''
                  example: translation map
                companyname:
                  type: string
                  description: 'Company name or trading partner name for translation. [optional] - wildcard character ''*'''
                  example: FreightTrust & Clearing Corporation
                partnername:
                  type: string
                  description: 'Partner for the translation map. [optional] - wildcard character ''*'''
                  example: Partner 2
                version:
                  type: string
                  description: 'Version of the translation map. [optional] - wildcard character ''*'''
                  example: '1.0'
                access_permission:
                  type: string
                  description: Access permission authorization level of the records to return.
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: public
                manage_permission:
                  type: string
                  description: Manage permission authorization level of the records to return.
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: company
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of grammars.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
