  /api/v1/grammar/list:
    post:
      tags:
        - Grammar
      summary: Gets a list of grammars.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  default: id
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                search_after:
                  type: array
                  description: 'Array of key/values to get the next set of result after. [optional]'
                  example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                access_permission:
                  type: string
                  description: 'Access permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: public
                manage_permission:
                  type: string
                  description: 'Manage permission authorization level of the records to return. [optional]'
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: company
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of grammars.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
                    properties:
                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      search_after_values:
                        type: array
                        description: 'Key/Value pairs for the last record, used to search for the next page of results.'
                        example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                  results:
                    type: array
                    description: List of grammar records.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: ID of the record.
                          example: hdP13WwBKlTkYuo1LBqy
                        name:
                          type: string
                          description: Name of the grammar.
                          example: x12-855-004010
                        companyname:
                          type: string
                          description: Company for the grammar..
                          example: ''
                        description:
                          type: string
                          description: Description of the grammar this record belongs to.
                          example: X12 004010 855 Grammar.
                        documenttype:
                          type: string
                          description: Type of document for this grammar.
                          example: '855'
                        version:
                          type: string
                          description: Version for the grammar.
                          example: '004010'
                        formattype:
                          type: string
                          description: Document format of the grammar.
                          example: x12
                        messagetype:
                          type: string
                          description: Name of the grammar message type.
                          example: '855004010'
  /api/v1/grammar/search:
    post:
      tags:
        - Grammar
      summary: Search grammars and return a list.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                name:
                  type: string
                  description: 'Name of the grammar. [optional] - wildcard character ''*'''
                  example: X12-850*
                formattype:
                  type: string
                  description: 'Format type of grammar. [optional] - wildcard character ''*'''
                  example: x12
                messagetype:
                  type: string
                  description: 'Message type of grammar. [optional] - wildcard character ''*'''
                  example: 850*
                documenttype:
                  type: string
                  description: 'Document type of grammar. [optional] - wildcard character ''*'''
                  example: '850'
                companyname:
                  type: string
                  description: 'Company name or trading partner name for the grammar. [optional] - wildcard character ''*'''
                  example: '850'
                version:
                  type: string
                  description: 'Version of the grammar. [optional] - wildcard character ''*'''
                  example: '850'
                access_permission:
                  type: string
                  description: Access permission authorization level of the records to return.
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: public
                manage_permission:
                  type: string
                  description: Manage permission authorization level of the records to return.
                  oneOf:
                    - public
                    - network
                    - company
                    - user
                  example: company
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of grammars.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
                    properties:
                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                  results:
                    type: array
                    description: List of grammar records.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: ID of the record.
                          example: hdP13WwBKlTkYuo1LBqy
                        name:
                          type: string
                          description: Name of the grammar.
                          example: x12-855-004010
                        companyname:
                          type: string
                          description: Company for the grammar..
                          example: ''
                        description:
                          type: string
                          description: Description of the grammar this record belongs to.
                          example: X12 004010 855 Grammar.
                        documenttype:
                          type: string
                          description: Type of document for this grammar.
                          example: '855'
                        version:
                          type: string
                          description: Version for the grammar.
                          example: '004010'
                        formattype:
                          type: string
                          description: Document format of the grammar.
                          example: x12
                        messagetype:
                          type: string
                          description: Name of the grammar message type.
                          example: '855004010'
