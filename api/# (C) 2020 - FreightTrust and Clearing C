                  type: object
                  description: 'Values of control numbers for tradacoms position records. [optional]'
                  properties:
                    general:
                      type: integer
                      format: int32
                      description: 'Current/default control number. [optional]'
                      example: 1
                    parter_id:
                      type: integer
                      format: int32
                      description: Current control number for a specific partner.
                      example: 0
                x12_control_number:
                  type: object
                  description: 'Values of control numbers for x12 position records. [optional]'
                  properties:
                    general:
                      type: integer
                      format: int32
                      description: 'Current/default control number. [optional]'
                      example: 1
                    parter_id:
                      type: integer
                      format: int32
                      description: Current control number for a specific partner.
                      example: 0
                xml_control_number:
                  type: object
                  description: 'Values of control numbers for xml position records. [optional]'
                  properties:
                    general:
                      type: integer
                      format: int32
                      description: 'Current/default control number. [optional]'
                      example: 1
                    parter_id:
                      type: integer
                      format: int32
                      description: Current control number for a specific partner.
                      example: 0
                permissions:
                  type: object
                  description: 'Permissions to assign to the record. [optional]'
                  properties:
                    access:
                      type: string
                      description: 'Access permission level for the record. [optional]'
                      example: public
                      oneOf:
                        - public
                        - network
                        - company
                        - user
                    manage:
                      type: string
                      description: 'Manage permission level for the record. [optional]'
                      example: user
                      oneOf:
                        - public
                        - network
                        - company
                        - user
      responses:
        '202':
          description: '(ACCEPTED) Successfully updated, returns updated result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    # TODO : CHANGE RECORD_ID TO FILE NAMING SCHEMA
                    example: idP43WwBKlTdf5o11xqE
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: updated
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
  /api/v1/convert/string:
    post:
      tags:
        - Convert
      summary: Converts/Translates data from a string.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                translate_id:
                  type: string
                  description: ID of the translation map to use for data conversion.
                  # TODO : CHANGE TO NAMESPACE SPEC FOR TRANSLATION MAPS
                  example: qeU5zmwBbw3B1k8xfVIj
                acknowledge:
                  type: integer
                  format: int32
                  description: 'Flag to create an acknowledgement or not.<br/> For EDI document types primarily. [optional]'
                  example: '0'
                  default: '0'
                base64:
                  type: integer
                  format: int32
                  description: 'Flag to mark string as base64 or not. [optional]'
                  example: '0'
                  default: '0'
                in_data:
                  type: string
                  description: Inbound data to convert.
                  example: <?xmlversion="1.0" encoding="utf-8"?><message><sender>PARTNERB</sender><receiver>SUPPLIER1</receiver>...
      responses:
        '200':
          description: '(OK) Successfully processed, returns data conversion results.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  acknowledgement:
                    type: string
                    format: base64
                    description: Base64 encoded string of the acknowledgement.
                    # TODO : NAMESPACE SPEC?
                    example: 
                  acknowledgement_text:
                    type: string
                    description: Text string of the acknowledgement.
                    example: ISA*00* *00* *ZZ*9339687055968 *ZZ*9377778093925 *200511*0659*U*00200*000543539*0*P*>
                  count:
                    type: integer
                    format: int32
                    description: Number of records the data was converted to.
                    example: 1
                  log_id:
                    type: string
                    description: Log ID record of the translation process.
                    # TODO: UUID/V5 
                    example:
                  data:
                    type: array
                    description: List of results from the data conversion.
                    items:
                      type: object
                      properties:
                        content:
                          type: string
                          format: base64
                          description: Base64 encoded string of the converted data.
                          example: SVNBKjAwKiAgICAgICAgICAqMDAq...
                        text_content:
                          type: string
                          description: Text string of the converted data.
                          example: ISA*00* *00* *ZZ*9339687055968 *ZZ*9377778093925 *200511*0659*U*00200*000543539*0*P*>
                        success:
                          type: boolean
                          description: If the conversion was successful.
                          example: true
                        metadata:
                          type: object
                          description: Metadata values of the conversion data.
                          properties:
                            from_content_type:
                              type: string
                              description: MIME Type of the inbound data.
                              example: application/json
                            from_format_type:
                              type: string
                              description: Data format of the inbound data.
                              example: jsonnocheck
                            from_message_type:
                              type: string
                              description: Message Type/Grammar name of the inbound data.
                              example: JSON_UBL_ORDER
                            from_partner:
                              type: string
                              description: Partner who sent the inbound data.
                              example: TESTPARTNER1
                            mapping_script:
                              type: string
                              description: Translation map to used.
                              example: JSON_2_850
                            reference:
                              type: string
                              description: Reference number or identifier of the inbound data.
                              example: '1234567890'
                            test_indicator:
                              type: string
                              description: Flag indicating test or production for the inbound data.
                              example: P
                            to_content_type:
                              type: string
                              description: MIME Type of the outbound data.
                              example: application/x12
                            to_format_type:
                              type: string
                              description: Data format of the outbound data.
                              example: x12
                            to_message_type:
                              type: string
                              description: Message Type/Grammar name of the outbound data.
                              example: '850004010'
                            to_partner:
                              type: string
                              description: Partner who sent the outbound data.
                              example: TESTPARTNER2
  /api/v1/convert/file:
    post:
      tags:
        - Convert
      summary: Converts data from a file.
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties:
                translate_id:
                  type: string
                  description: ID of the translation map to use for data conversion.
                  # TODO : TRANSLATION MAPING SPECIFICATION 
                  example: 
                out_filename:
                  type: string
                  description: The name of the outbound file.
                  example: some-file.json
                in_file:
                  type: string
                  format: binary
                  description: The file to convert.
      responses:
        '200':
          description: '(OK) Successfully processed, return the converted data file.'
          content:
            application/octet-stream:
              schema:
                type: file
                format: binary
