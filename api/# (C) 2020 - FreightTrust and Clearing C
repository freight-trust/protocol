                    example: true
  /api/v1//grammar/htmltemplate/file:
    post:
      tags:
        - Grammar
      summary: Add a grammar from a file.
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties:
                name:
                  type: string
                  description: 'Name of the grammar. [optional]'
                  example: Order HTML Template Envelope
                companyname:
                  type: string
                  description: 'Company for the grammar. [optional]'
                  example: ''
                description:
                  type: string
                  description: 'Description of the grammar this record belongs to. [optional]'
                  example: Orde HTML Envelope Template Grammar.
                documenttype:
                  type: string
                  description: 'Type of document for this grammar. [optional]'
                  example: html
                version:
                  type: string
                  description: 'Version for the grammar. [optional]'
                  example: '1'
                formattype:
                  type: string
                  description: Document format of the grammar.
                  example: templatehtml
                messagetype:
                  type: string
                  description: Name of the grammar message type.
                  example: orders2printenvelope
                file:
                  type: string
                  format: binary
                  description: The file to create a grammar template for.
                permissions:
                  type: object
                  description: Permissions to assign to the record.
                  properties:
                    access:
                      type: string
                      description: Access permission level for the record.
                      example: public
                      oneOf:
                        - public
                        - network
                        - company
                        - user
                    manage:
                      type: string
                      description: Manage permission level for the record.
                      example: user
                      oneOf:
                        - public
                        - network
                        - company
                        - user
      responses:
        '201':
          description: '(CREATED) Successfully created, returns operation result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: idP43WwBKlTdf5o11xqE
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: created
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
  '/api/v1/grammar/{id}':
    parameters:
      - name: id
        in: path
        description: ID of the record that we want to match
        type: string
    get:
      tags:
        - Grammar
      summary: Get a record with given ID. Must have appropriate access permission.
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      responses:
        '200':
          description: '(OK) Successfully created, returns the result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  name:
                    type: string
                    description: Name of the grammar.
                    example: x12-855-004010
                  companyname:
                    type: string
                    description: Company for the grammar.
                    example: ''
                  description:
                    type: string
                    description: Description of the grammar this record belongs to.
                    example: X12 004010 855 Grammar.
                  documenttype:
                    type: string
                    description: Type of document for this grammar.
                    example: '855'
                  version:
                    type: string
                    description: Version for the grammar.
                    example: '004010'
                  formattype:
                    type: string
                    description: Document format of the grammar.
                    example: x12
                  messagetype:
                    type: string
                    description: Name of the grammar message type.
                    example: '855004010'
                  content:
                    type: string
                    format: base64
                    description: Base64 encode string of the grammar.
                    example: ZnJvbSBib3RzY29uZmlnIG...
                  permissions:
                    type: object
                    description: Permissions to assign to the record.
                    properties:
                      access:
                        type: string
                        description: Access permission level for the record.
                        example: public
                        oneOf:
                          - public
                          - network
                          - company
                          - user
                      manage:
                        type: string
                        description: Manage permission level for the record.
                        example: user
                        oneOf:
                          - public
                          - network
                          - company
                          - user
    put:
      summary: Edit/Update a record by ID. Must have appropriate access permission.
      tags:
        - Grammar
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      requestBody:
        description: Control Number Object
        content:
          application/json:
            schema:
              type: object
              properties:
                name:
                  type: string
                  description: 'Name of the grammar. [optional]'
                  example: x12-855-004010
                companyname:
                  type: string
                  description: 'Company for the grammar. [optional]'
                  example: ''
                description:
                  type: string
                  description: 'Description of the grammar this record belongs to. [optional]'
                  example: X12 004010 855 Grammar.
                documenttype:
                  type: string
                  description: 'Type of document for this grammar. [optional]'
                  example: '855'
                version:
                  type: string
                  description: 'Version for the grammar. [optional]'
                  example: '004010'
                formattype:
                  type: string
                  description: 'Document format of the grammar. [optional]'
                  example: x12
                messagetype:
                  type: string
                  description: 'Name of the grammar message type. [optional]'
                  example: '855004010'
                content:
                  type: string
                  format: base64
                  description: 'Base64 encode string of the grammar. [optional]'
                  example: ZnJvbSBib3RzY29uZmlnIG...
                permissions:
                  type: object
                  description: 'Permissions to assign to the record. [optional]'
                  properties:
                    access:
                      type: string
                      description: 'Access permission level for the record. [optional]'
                      example: public
                      oneOf:
                        - public
                        - network
                        - company
                        - user
                    manage:
                      type: string
                      description: 'Manage permission level for the record. [optional]'
                      example: user
                      oneOf:
                        - public
                        - network
                        - company
                        - user
      responses:
        '202':
          description: '(ACCEPTED) Successfully updated, returns updated result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: idP43WwBKlTdf5o11xqE
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: updated
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
    delete:
      summary: Delete record by ID. Must have appropriate access permission.
      tags:
        - Grammar
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      responses:
        '200':
          description: '(OK) Successfully deleted, returns deleted result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: idP43WwBKlTdf5o11xqE
