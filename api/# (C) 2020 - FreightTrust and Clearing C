components:
  securitySchemes:
    ApiKeyAuth:
      type: apiKey
      in: header
      name: X-API-KEY
      description: 'API key to authorize requests. HTTP Header = X-API-KEY : <apikey>'
    BasicAuth:
      type: http
      scheme: basic
      description: 'Basic authentication to authorize requests. HTTP Header = Authorization : Basic base64-encode(''username:password'')'
  schemas:
    ErrorObject:
      type: object
      description: All responses that do not return a 2XX HTTP Status Code return this Error Object.
      properties:
        success:
          type: boolean
          example: false
        error:
          type: object
          properties:
            type:
              type: string
              description: HTTP Status Code and Description
              example: 404 - Not Found
            message:
              type: string
              description: Additional message or description of the error
              example: This entity is not found