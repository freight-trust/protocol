      tags:
        - Translation Map
      summary: Get a record with given ID and return a file. Must have appropriate access permission.
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      responses:
        '200':
          description: '(OK) Successfully processed, return the translation map file.'
          content:
            application/octet-stream:
              schema:
                type: file
                format: binary
  /api/v1/user/list:
    post:
      tags:
        - User
      summary: Gets a list of users. Requires manage-user user permissions.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                from:
                  type: integer
                  format: int32
                  description: 'Record number to start the list from. [optional]'
                  default: 0
                  example: 0
                size:
                  type: integer
                  format: int32
                  description: 'Number of records to return. [optional]'
                  default: 100
                  example: 100
                  minimum: 0
                  maximum: 10000
                sort_by:
                  type: string
                  description: 'The property to sor the list by. [optional]'
                  default: id
                  example: name
                sort_order:
                  type: string
                  description: 'the order in which to sort the list. [optional]'
                  oneOf:
                    - asc
                    - desc
                  example: asc
                search_after:
                  type: array
                  description: 'Array of key/values to get the next set of result after. [optional]'
                  example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
      responses:
        '200':
          description: '(OK) Successfully processed, returns list of translation maps.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  meta:
                    type: object
                    properties:
                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      search_after_values:
                        type: array
                        description: 'Key/Value pairs for the last record, used to search for the next page of results.'
                        example: '["search_after":["BOPBNKD12AUN", "rXuYp20B8ihBaWlEFvqS"]'
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                  results:
                    type: array
                    description: List of records.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: The ID of the authorized user.
                          example: jx8Yq2wOMt0esONO7uLx
                        user_name:
                          type: string
                          description: The username of the authorized user.
                          example: JohnDoe@Company.com
                        name:
                          type: string
                          description: The name of the authorized user.
                          example: John Doe
                        company_name:
                          type: string
                          description: The company the authorized user belongs to.
                          example: FreightTrust & Clearing Corporation
                        company_id:
                          type: string
                          description: The Company ID of the authorized user.
                          example: Q5GfiOSq566GIydfguBfg0rD
                        alt_email_1:
                          type: string
                          description: The alternate email 1 of the authorized user.
                          example: name1@company.com
                        alt_email_2:
                          type: string
                          description: The alternate email 2 of the authorized user.
                          example: name2@company.com
                        phone:
                          type: string
                          description: The phone number of the authorized user.
                          example: 555-123-4567
                        department:
                          type: string
                          description: The department of the authorized user.
                          example: Sales
                        position:
                          type: string
                          description: The position of the authorized user.
                          example: Manager
                        roles:
                          type: array
                          description: The permission roles of the authorized user.
                          example: '[manage-users, manage-company]'
  /api/v1/user/add:
    post:
      tags:
        - User
      summary: Add a user record.
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                user_name:
                  type: string
                  description: The username of the authorized user.
                  example: JohnDoe@Company.com
                name:
                  type: string
                  description: 'The name of the authorized user. [optional]'
                  example: John Doe
                alt_email_1:
                  type: string
                  description: 'The alternate email 1 of the authorized user. [optional]'
                  example: name1@company.com
                alt_email_2:
                  type: string
                  description: 'The alternate email 2 of the authorized user. [optional]'
                  example: name2@company.com
                phone:
                  type: string
                  description: 'The phone number of the authorized user. [optional]'
                  example: 555-123-4567
                department:
                  type: string
                  description: 'The department of the authorized user. [optional]'
                  example: Sales
                position:
                  type: string
                  description: 'The position of the authorized user. [optional]'
                  example: Manager
                password:
                  type: string
                  description: 'Password for the authorized user. [optional]'
                  example: 1
                roles:
                  type: array
                  description: 'User permission roles. [optional]'
                  oneOf:
                    - manage-user
                    - manage-billing
                    - manage-company
      responses:
        '201':
          description: '(CREATED) Successfully created, returns add result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: idP43WwBKlTdf5o11xqE
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: created
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
  '/api/v1/user/{id}':
    parameters:
      - name: id
        in: path
        description: ID of the record that we want to match
        type: string
    put:
      summary: Edit/Update a record by ID. Must have manage-user permissions.
      tags:
        - User
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      requestBody:
        description: User Object
        content:
          application/json:
            schema:
              type: object
              properties:
                user_name:
                  type: string
                  description: The username of the authorized user.
                  example: JohnDoe@Company.com
                name:
                  type: string
                  description: 'The name of the authorized user. [optional]'
                  example: John Doe
                company_name:
                  type: string
                  description: 'The name of the company. [optional]'
                  example: Company B
                alt_email_1:
                  type: string
                  description: 'The alternate email 1 of the authorized user. [optional]'
                  example: name1@company.com
                alt_email_2:
                  type: string
                  description: 'The alternate email 2 of the authorized user. [optional]'
                  example: name2@company.com
                phone:
                  type: string
                  description: 'The phone number of the authorized user. [optional]'
                  example: 555-123-4567
                department:
                  type: string
                  description: 'The department of the authorized user. [optional]'
                  example: Sales
                position:
                  type: string
                  description: 'The position of the authorized user. [optional]'
                  example: Manager
                active:
                  type: integer
                  description: 'Flag for if the authorized user is active. [optional]'
                  example: 1
                password:
                  type: string
                  description: 'Password for the authorized user. [optional]'
                  example: 1
                roles:
                  type: array
                  description: 'User permission roles. [optional]'
                  oneOf:
                    - manage-user
                    - manage-billing
                    - manage-company
      responses:
        '202':
          description: '(ACCEPTED) Successfully updated, returns updated result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: idP43WwBKlTdf5o11xqE
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: updated
                  success:
                    type: boolean
                    description: If the operation was successful.
                    example: true
    delete:
      summary: Delete record by ID. Must have appropriate permission.
      tags:
        - User
      parameters:
        - in: path
          name: id
          description: Record ID
          schema:
            $ref: '#/components/parameters/id'
      responses:
        '200':
          description: '(OK) Successfully deleted, returns deleted result.'
          content:
            application/json:
              schema:
                type: object
                properties:
                  id:
                    type: string
                    description: The ID of the record
                    example: idP43WwBKlTdf5o11xqE
                  result:
                    type: string
                    description: Text description of the record operation.
                    example: deleted
