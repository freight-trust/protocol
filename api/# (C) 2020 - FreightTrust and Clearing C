                      count:
                        type: integer
                        format: int32
                        description: Number of records returned.
                        example: 10
                      total:
                        type: integer
                        format: int32
                        description: Total number of records.
                        example: 100345
                  results:
                    type: array
                    description: List of grammar records.
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                          description: ID of the record.
                          example: hdP13WwBKlTkYuo1LBqy
                        companyname:
                          type: string
                          description: Company for the translation map.
                          example: Partner 2
                        partnername:
                          type: string
                          description: Partner for the translation map.
                          example: Partner 2
                        description:
                          type: string
                          description: Description of the translation map this record belongs to.
                          example: Converts ERP Order CSV File to X12 850 004010
                        documenttype:
                          type: string
                          description: Type of document for this translation map.
                          example: Translation map
                        version:
                          type: string
                          description: Version for the translation map.
                          example: '1.0'
                        from_formattype:
                          type: string
